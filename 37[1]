import math

def count_vowel_strings(n):
    return math.comb(n + 4, 4)

# Test cases
print(count_vowel_strings(1))  # Output: 5
print(count_vowel_strings(2))  # Output: 15
print(count_vowel_strings(3))  # Output: 35
print(count_vowel_strings(4))  # Output: 70
print(count_vowel_strings(5))  # Output: 126
