def sumsquare(l):
    odd_sum = 0
    even_sum = 0
    for num in l:
        if num % 2 == 0:
            even_sum += num ** 2
        else:
            odd_sum += num ** 2
    return [odd_sum, even_sum]

# Sample input and output
elements = [18, 9, 1, 12, 13, 4, 30]
print(sumsquare(elements))  # Output: [251, 1384]
